{
  "compilerOptions": {
    "target": "es2022",
    "module": "esnext",
    "rootDir": "./src",
    "outDir": "./dist",
    "removeComments": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}